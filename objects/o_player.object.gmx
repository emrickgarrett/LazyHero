    // Gravity
    if(place_free(x, y+1)){
        //sprite_index = s_jumping;
        gravity = .75;
        
        //Set max fall speed
        if(vspeed &gt;= 10)
            vspeed = 10;    
    }else{
        gravity = 0;
        
        //Jumping
        if(up){
            vspeed = -15;
        }
    }
    
}else{
